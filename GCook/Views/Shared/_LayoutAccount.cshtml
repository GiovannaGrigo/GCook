<!DOCTYPE html>
<html lang="pt-br">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">
    <meta name="generator" content="">
    <title>GCook - @ViewData["Title"]</title>

    <!-- Favicons -->
    <link rel="icon" href="~/img/logo.png" type="image/png">

    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/css/admin.css">
    <link rel="stylesheet" href="~/css/login.css">

</head>

<body class="bg-dark">
    <div class="container">
        <div class="text-center text-white mb-4">
            <a asp-action="Index" asp-controller="Home" class="text-decoration-none">
                <img src="~/img/logo.png" alt="Logo" width="180" height="180" class="mb-2">
            </a>
            <p class="h3 mt-2">@ViewData["SubTitle"]</p>
        </div>

        @RenderBody();

    </div>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <partial name="_ValidationScriptsPartial"></partial>
    <script src="//cdn.jsdeliver.net/npm/sweetalert2@11"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>

</html>